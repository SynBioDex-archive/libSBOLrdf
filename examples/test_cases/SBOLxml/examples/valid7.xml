<?xml version="1.0"?>
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:so="http://purl.obolibrary.org/obo/" 
	xmlns:s="http://sbols.org/v1#"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sbols.org/v1# ../schema/sbol.xsd http://www.w3.org/1999/02/22-rdf-syntax-ns# ../schema/rdf.xsd">
	
	<s:DnaComponent rdf:about="http://example.com/dc1">
		<s:displayId>DC1</s:displayId>
		<s:name>DnaComponent1</s:name>
		<s:description>Sequence annotations can refer to use DNA components by reference too</s:description>
		<s:dnaSequence rdf:resource="test">
			<s:DnaSequence rdf:about="http://example.com/ds1">
				<s:nucleotides>tccctatcagtgat</s:nucleotides>
			</s:DnaSequence>
		</s:dnaSequence>
		<s:annotation>
			<s:SequenceAnnotation rdf:about="example.com/sa1">
				<s:subComponent rdf:resource="http://example.com/dc2"/>
			</s:SequenceAnnotation>
		</s:annotation>	
	</s:DnaComponent>
	
	<s:DnaComponent rdf:about="http://example.com/dc2">
		<s:displayId>DC2</s:displayId>
		<s:name>DnaComponent2</s:name>
		<s:description>This is a sub component</s:description>
	</s:DnaComponent>
	
</rdf:RDF>

