   <s:DnaComponent rdf:about="http://example.com/dc1">
      <s:displayId>DC1</s:displayId>
      <s:name>DnaComponent1</s:name>
      <s:description>Invalid bioStart value (not a positive integer)</s:description>
      <s:annotation>
         <s:SequenceAnnotation rdf:about="http://example.com/sa2">
            <s:bioStart>-1</s:bioStart>
            <s:bioEnd>54</s:bioEnd>
            <s:strand>+</s:strand>
            <s:subComponent>
               <s:DnaComponent rdf:about="http://example.com/dc2">
                  <s:displayId>DC2</s:displayId>
                  <s:name>DnaComponent2</s:name>
                  <s:description>Another DNA component</s:description>
               </s:DnaComponent>
            </s:subComponent>
         </s:SequenceAnnotation>
      </s:annotation>
   </s:DnaComponent>

