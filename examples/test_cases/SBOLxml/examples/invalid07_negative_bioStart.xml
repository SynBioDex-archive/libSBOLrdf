<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:so="http://purl.obolibrary.org/obo/" xmlns:s="http://sbols.org/v1#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
